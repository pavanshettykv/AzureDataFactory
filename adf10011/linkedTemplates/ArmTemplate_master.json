{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf10011"
		},
		"ls_AmazonS3_secretAccessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'secretAccessKey' of 'ls_AmazonS3'"
		},
		"ls_AzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_AzureBlobStorage'"
		},
		"ls_AzureDataLakeStorage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_AzureDataLakeStorage'"
		},
		"ls_gcp_oder_items_secretAccessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'secretAccessKey' of 'ls_gcp_oder_items'"
		},
		"ls_AmazonS3_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "AKIATCKAQMCPA7ZBHB7G"
		},
		"ls_AzureDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlkaccount.dfs.core.windows.net/"
		},
		"ls_gcp_oder_items_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "GOOG3XL2A6HHIZONL3U4FY54"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf10011_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_AmazonS3_secretAccessKey": {
						"value": "[parameters('ls_AmazonS3_secretAccessKey')]"
					},
					"ls_AzureBlobStorage_connectionString": {
						"value": "[parameters('ls_AzureBlobStorage_connectionString')]"
					},
					"ls_AzureDataLakeStorage_accountKey": {
						"value": "[parameters('ls_AzureDataLakeStorage_accountKey')]"
					},
					"ls_gcp_oder_items_secretAccessKey": {
						"value": "[parameters('ls_gcp_oder_items_secretAccessKey')]"
					},
					"ls_AmazonS3_properties_typeProperties_accessKeyId": {
						"value": "[parameters('ls_AmazonS3_properties_typeProperties_accessKeyId')]"
					},
					"ls_AzureDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('ls_AzureDataLakeStorage_properties_typeProperties_url')]"
					},
					"ls_gcp_oder_items_properties_typeProperties_accessKeyId": {
						"value": "[parameters('ls_gcp_oder_items_properties_typeProperties_accessKeyId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}